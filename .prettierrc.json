{
  "plugins": [
    "prettier-plugin-organize-imports"
  ],
  "singleQuote": true,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "trailingComma": "all",
  "jsxBracketSameLine": false,
  "singleAttributePerLine": false,
  "printWidth": 120,
  "bracketSpacing": true,
  "arrowParens": "always",
  "bracketSameLine": true
}
